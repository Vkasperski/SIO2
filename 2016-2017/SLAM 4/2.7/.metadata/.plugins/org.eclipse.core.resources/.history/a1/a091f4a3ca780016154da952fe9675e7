public interface Annimal 
{
	public void setNom();
	public String getNom();
	public String parle();
	
	class Chat implements Annimal
	{
		private
			String Cnom;
			String Cnom2;
		public Chat(String nom)
		{
			Cnom=nom;
		}
		@Override
		public void setNom() 
		{
			Cnom=Cnom2;
		}

		@Override
		public String getNom() 
		{
			return Cnom;
		}

		@Override
		public String parle() 
		{
			return "MIAOUUUUUUUS!!!!";
		}
		
	}
	
	class Chien implements Annimal
	{
		private
		String Cinom;
		String Cinom2;
		@Override
		public void setNom() 
		{
			Cinom=Cinom2;
		}

		@Override
		public String getNom() 
		{
			return Cinom;
		}

		@Override
		public String parle() 
		{
			
			return "ARCANIIIIIIIIN !!!";
		}
	}
	
	class Vache implements Annimal
	{

		@Override
		public void setNom() 
		{
			
		}

		@Override
		public String getNom() 
		{
			
			return null;
		}

		@Override
		public String parle() 
		{
			
			return null;
		}
	}
}
