interface Saluer
{
  
}

class Bonjour implements Saluer
{
        public void saluer()
        {
                System.out.println("Bonjour");
        }
}

class Hello implements Saluer
{
        public void saluer()
        {
                System.out.println("Hello");
        }
}

class GutenTag implements Saluer
{
        public void saluer()
        {
                System.out.println("Guten tag");
        }
}

class DzienDobry implements Saluer
{
	public void saluer()
	{
		 System.out.println("Dzien Dobry");
	}
}
public class ExempleInterface
{
        public static void main(String[] args)
        {
                Saluer s = new Bonjour();
                ((Bonjour)s).saluer();
                s = new Hello();
                ((Hello)s).saluer();
                Saluer[] t = new Saluer[] { new Bonjour(), new Hello(), new GutenTag() };
                for (int i = 0; i < 3; i++)
                        ((Bonjour) t[i]).saluer();
                s = new DzienDobry();
                ((DzienDobry)s).saluer();
        }
}